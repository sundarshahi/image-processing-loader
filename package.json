{
  "name": "@sundarshahi/image-processing-loader",
  "private": false,
  "description": "A library for image processing loader",
  "version": "0.0.10",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "check-publish": "npm publish --dry-run"
  },
  "dependencies": {
    "@sundarshahi/image-processing-loader": "^0.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite-plugin-dts": "^3.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-test-renderer": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jsdom": "^24.1.1",
    "postcss": "^8.4.41",
    "react-test-renderer": "^18.3.1",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sundarshahi/image-processing-loader.git"
  },
  "keywords": [
    "image-processing-loader",
    "loaders",
    "animated-loader"
  ],
  "author": {
    "name": "Sundar Shahi Thakuri",
    "url": "https://github.com/sundarshahi",
    "email": "shahithakurisundar@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/sundarshahi/image-processing-loader/issues"
  },
  "homepage": "https://github.com/sundarshahi/image-processing-loader#readme",
  "license": "MIT"
}
